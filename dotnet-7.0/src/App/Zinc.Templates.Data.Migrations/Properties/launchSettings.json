      "commandName": "Project",
      "dotnetRunMessages": "true",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "APP_ENTRYPOINT": "Zinc.Templates.Data.Migrations",
        "ApplicationContext__ApplicationName": "Zinc.Templates",
