        "ASPNETCORE_Kestrel__Certificates__Default__Path": "%RL_DOCKER_VOLS_ROOT%/in/certs/redline.pfx",
        "Authentication__SigningCert__Path": "%RL_DOCKER_VOLS_ROOT%/in/certs/redline.pfx",
        "Authentication__DataProtectionCert__Path": "%RL_DOCKER_VOLS_ROOT%/in/certs/redline.pfx",
        "A3__Authentication__PublicKeyPath": "%RL_DOCKER_VOLS_ROOT%/in/certs/redline-public.crt"
      }
    }
  }
}
